<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.tryno4.Entity.AdressesEntity" table="adresses" schema="adress">
        <id name="idAdresses">
            <column name="id_adresses" sql-type="int"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="adress">
            <column name="adress" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>